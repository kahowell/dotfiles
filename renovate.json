{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^versions.yaml$"],
      "matchStrings": [
        "#\\s?renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)\\s?[\\w+_]*:\\s?[\"']?(?<currentValue>[\w+\.\-]*)[\"']"
      ]
    }
  ]
}
